{
  "name": "@mvccore/ext-controller-datagrid-ag-js",
  "version": "5.2.18",
  "description": "MvcCore controller datagrid extension for administration environments with AgGrid JS/TS front-end.",
  "author": {
    "name": "Tom Fl√≠dr",
    "email": "tomflidr@gmail.com",
    "url": "https://github.com/tomFlidr"
  },
  "license": "BSD-3",
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/mvccore/ext-controller-datagrid-ag-js.git"
  },
  "scripts": {
    "prepublish": "npm run build",
    "build": "tsc && copy-files-from-to"
  },
  "copyFiles": [
    {
      "from": "./types/agGrid.d.ts",
      "to": "../ext-controller-datagrid-ag/assets/declarations/agGrid.d.ts"
    }
  ],
  "copyFilesSettings": {
    "whenFileExists": "overwrite",
    "removeSourceMappingURL": false,
    "uglifyJs": false,
    "addReferenceToSourceOfOrigin": false,
    "ignoreDotFilesAndFolders": true
  },
  "dependencies": {
    "ag-grid-community": "^28.2.1",
    "ajax-min": "^1.0.13",
    "moment": "^2.29.4",
    "tslib": "^2.4.0"
  },
  "devDependencies": {
    "copy-files-from-to": "^3.6.0",
    "typescript": "^5.0.4"
  }
}